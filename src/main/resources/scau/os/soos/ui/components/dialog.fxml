<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="body" minHeight="-Infinity" minWidth="-Infinity" nodeOrientation="LEFT_TO_RIGHT" styleClass="dialog" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@../../css/common.css" />
        <URL value="@../../css/icon.css" />
    </stylesheets>
   <bottom>
      <HBox alignment="BOTTOM_RIGHT" fillHeight="false" spacing="16.0" styleClass="bottom" BorderPane.alignment="CENTER">
         <children>
            <Button id="cancel-btn" mnemonicParsing="false" styleClass="outline-btn" text="取消" />
            <Button id="confirm-btn" mnemonicParsing="false" styleClass="primary-btn" text="确认" />
         </children>
      </HBox>
   </bottom>
   <top>
      <Label id="dialog-title" styleClass="title" text="确认删除 man 吗？" BorderPane.alignment="TOP_LEFT" />
   </top>
</BorderPane>
