<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="body"
            xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            styleClass="dialog">
    <stylesheets>
        <URL value="@../../css/common.css" />
        <URL value="@../../css/icon.css" />
    </stylesheets>
   <bottom>
      <HBox styleClass="bottom" alignment="BOTTOM_RIGHT" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="cancel-btn" mnemonicParsing="false" text="取消" />
            <Button id="confirm-btn" mnemonicParsing="false" text="确认" />
         </children>
         <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
