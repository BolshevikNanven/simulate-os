<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="180.0" prefWidth="150.0"
      stylesheets="@../css/ThumbnailBox.css"
      xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.od.imageview.controller.ThumbnailBoxController">
   <children>
      <StackPane prefHeight="150.0" prefWidth="150.0">
         <ImageView fx:id="thumbnail" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" />
      </StackPane>
      <Label fx:id="thumbnailLabel" alignment="CENTER" prefHeight="30.0" prefWidth="150.0"/>
   </children>
</VBox>
